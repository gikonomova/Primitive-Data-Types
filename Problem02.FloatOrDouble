// Primitive-Data-Types
// Programming Basics September

using System;

class FloatOrDouble
{
    static void Main()
    {
        double first = 34.567839023;
        float second = 12.345f;
        double third = 8923.1234857;
        float fourth = 3456.091f;

        Console.WriteLine("{0}\n{1}\n{2}\n{3}", first, second, third, fourth);
        Console.ReadLine();
    }
}

